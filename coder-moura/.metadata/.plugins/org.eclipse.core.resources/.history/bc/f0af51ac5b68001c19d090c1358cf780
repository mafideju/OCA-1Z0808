package H_exception;

public class ChecadaeNaoChecada {

	public static void main(String[] args) {
		
		geraErro1();
		
		try {
			geraErro2();
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
	
	private static void geraErro1() {
		throw new RuntimeException();
	}

	private static void geraErro2() throws Exception {
		throw new Exception();
	}
}
